#include <stdio.h>
int
main (void)
{
  /* This is a very simple algorithm to find the factors of a number. */
  long n, factor, otherfactor;
  printf ("Number: ");
  scanf ("%ld", &n);
  factor = 1;
  otherfactor = 0;
  while (factor < n)
    {
      if (n % factor == 0)
        {
          otherfactor = n / factor;
          printf ("%ld Ã— %ld\n", factor, otherfactor);
        }
      factor += 1;
    }
  return 0;
}
